<ul class="feed-items">
	<% items.forEach(function(i) { %>
		<li>
			<h2 title="<%= i.guid %>"><%= i.title %></h2>
			<% if(i.imageUrl && i.imageUrl.length > 0) { %>
				<img src="<%= i.imageUrl %>" />
			<% } %>
			<div><%- i.body %></div>
		</li>
	<% }) %>
</ul>

<pre><%= JSON.stringify(items.map(function(i) { i.body = null; return i; })) %></pre>

